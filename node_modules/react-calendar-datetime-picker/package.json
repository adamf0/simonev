{
  "name": "react-calendar-datetime-picker",
  "version": "1.7.5",
  "description": "A fast and small calendar for react ",
  "author": "MehdiNasiri",
  "license": "MIT",
  "repository": "https://github.com/mmehdinasiri/react-calendar-datetime-picker",
  "homepage": "https://mmehdinasiri.github.io/react-calendar-datetime-picker/",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.tsx",
  "files": [
    "dist/"
  ],
  "keywords": [
    "persian date picker",
    "date picker",
    "date and time picker",
    "calendar date picker",
    "persian date time picker"
  ],
  "scripts": {
    "init:husky": "npx husky-init && husky install && husky set .husky/pre-commit 'npm run typecheck'",
    "dev:sample": "cd ./sample && npm run dev",
    "dev": "concurrently --kill-others \"npm run vite:dev\" \"npm run dev:sample\" ",
    "delete-dist": "rimraf ./dist",
    "delete-cjs-file": "rimraf ./dist/index.js",
    "start": "vite",
    "vite:dev": "tsc && vite build --watch --minify false --emptyOutDir false --config vite.config.development.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:latest": "npm run build && npm run preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:styles": "stylelint --allow-empty-input \"**/*.scss\"",
    "lint:fix": "npm run lint:fix-ts && npm run lint:fix-styles",
    "lint:fix-ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:fix-styles": "stylelint --allow-empty-input \"**/*.scss\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit",
    "push": "git push --follow-tags",
    "release": "standard-version",
    "release:first": "npm run release -- --first-release && npm run push",
    "release:major": "npm run release -- --release-as major && npm run push",
    "release:minor": "npm run release -- --release-as minor && npm run push",
    "release:notag": "npm run release -- --no-git-tag-version && npm run push",
    "release:patch": "npm run release -- --release-as patch && npm run push"
  },
  "peerDependencies": {
  "react": ">=18.0.0",
  "react-dom": ">=18.0.0"
},
  "dependencies": {
    "jalaali-js": "^1.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/jalaali-js": "^1.1.3",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.85.0",
    "standard-version": "^9.5.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.6"
  }
}
